<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>R1</servlet-name>
		<servlet-class>package1.StaffCheck</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>R2</servlet-name>
		<servlet-class>package1.staffDeletePage</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>R3</servlet-name>
		<servlet-class>package1.deleteProduct</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>R6</servlet-name>
		<servlet-class>package1.customerCheck</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>R7</servlet-name>
		<servlet-class>package1.customerAdd</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>R1</servlet-name>
		<url-pattern>/StaffCheck</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>R2</servlet-name>
		<url-pattern>/staffDeletePage</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>R3</servlet-name>
		<url-pattern>/deleteProduct</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>R6</servlet-name>
		<url-pattern>/customerCheck</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>R7</servlet-name>
		<url-pattern>/customerAdd</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>staffLogin.jsp</welcome-file>
		<welcome-file>staffDashboard.jsp</welcome-file>
		<welcome-file>staffDeletePage.java</welcome-file>
		<welcome-file>custLoginReg.jsp</welcome-file>
	</welcome-file-list>
</web-app>